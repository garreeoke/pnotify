# PNOTIFY_PASSWORD_EXPIRE_DAYS: number of days until expiration to send email to the user
export PNOTIFY_PASSWORD_EXPIRE_DAYS_THRESHOLD=900
# PNOTIFY_PASSWORD_INACTIVE_DAYS: number of days inactive to start sending emails
export PNOTIFY_PASSWORD_INACTIVE_DAYS_THRESHOLD=900
# PNOTIFY_SEND_EMAILS: true or false whether to send emails or not
export PNOTIFY_SEND_EMAILS=true
# PNOTIFY_REPORT_EMAILS: list of email addresses to send summary email
export PNOTIFY_REPORT_EMAILS=("garreesett@gmail.com" "torgerson.aaron@gmail.com" ) 
# PNOTIFY_DATA_FILE: relative path to input file
export PNOTIFY_DATA_FILE=data/user_data.csv
# PNOTIFY_OUTPUT_DIR: directory where summary reports will go
export PNOTIFY_OUTPUT_DIR=reports
